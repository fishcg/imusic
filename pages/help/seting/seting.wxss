.liimg{
  width:100%;
  height: 100%;
}
.back{
  position: fixed;
  margin-top: 30rpx;
  margin-left: 30rpx;
  padding: 5rpx 15rpx 5rpx 15rpx;
  border:1px solid #f1f1f1;
  border-radius: 10%;
  z-index: 99;
  color: #fff;
  font-size: 14px;
  text-shadow:0px 0px 1px #5acece;
  box-shadow:0px 0px 1px #5acece;
}
 page .wx-swiper-dots.wx-swiper-dots-horizontal{
      margin-bottom: -4rpx;
 } 
  page .wx-swiper-dot{
      width: 100rpx;
      display: inline-flex;
      height: 18rpx;
     vertical-align: sub; 
 }
 page .wx-swiper-dot::before{
     content: '';
     background: #e5e5e5;
     flex-grow: 1;
 }
 page .wx-swiper-dot-active::before{
     content: '';
     background:#5acece;
     flex-grow: 1;
 }